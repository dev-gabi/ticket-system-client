{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "categories",
      "installMode": "lazy",
      "updateMode": "lazy",
      "urls": [
        "https://localhost:44388/api/tickets/get-categories"
      ],
      "cacheConfig": {
        "maxSize": 5,
        "maxAge": "1d",
        "timeout": "10s",
        "strategy": "performance"
      }
    }

  ]
}
